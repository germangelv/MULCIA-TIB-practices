diagramaPuntos <- function(s1,s2){
r1 <- c()
r2 <- c()
ls1 <- length(s1)
ls2 <- length(s2)
for (i1 in 1:ls1) {
for (i2 in 1:ls2) {
if(s1[i1]==s2[i2]){
r1 <- c(r1,i1)
r2 <- c(r2,i2)
}
}
}
res <- data.frame(X=r1, Yr=r2)
plot(res, xlim=c(1,ls1), ylim=c(1,ls2),
xaxt="n",
yaxt="n",
pch=".",
col="blue"
)
axis(side=1, at=1:ls1, label=1:ls1)
axis(side=2, at=1:ls2, label=1:ls2)
}
diagramaPuntos(s1,s2)
